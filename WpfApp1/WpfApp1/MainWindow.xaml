                    <Button x:Name="NewAnim" Content="NewAnimal" Click="NewAnim_Click" FontSize="8" Margin="10,10,169.598,7"/>
                    <Button x:Name="NewTree" Content="NewTreeas" Click="NewTree_Click" FontSize="8" Margin="68,10,111.598,7"/>
                    <Button x:Name="None" Content="None" Click="None_Click" FontSize="8" Margin="126,11,53.598,6"/>
                </Grid>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
