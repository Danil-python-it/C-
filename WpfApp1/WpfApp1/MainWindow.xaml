<Window x:Name="MainWindow1" x:Class="шещп.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:шещп"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Activated="MainWindow1_Activated" MouseDown="MainWindow1_MouseDown" Background="#FF10239B">
    <Grid x:Name="can">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <TabControl HorizontalAlignment="Left" Margin="0,324,0,0" Width="293">
            <TabItem Header="TypeKyst" Background="#FFECB012">
                <Grid Background="#FFE5E5E5">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="8*"/>
                        <ColumnDefinition Width="55*"/>
                        <ColumnDefinition Width="54*"/>
                        <ColumnDefinition Width="57*"/>
                        <ColumnDefinition Width="113*"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="7*"/>
